#ifndef _KUPOLICY_NETLINK_VERSION_H
#define _KUPOLICY_NETLINK_VERSION_H

/** major netlink version */
#define UP_NL_VERMAJOR @UPOLICY_NL_VERMAJOR@
/** minor netlink version */
#define UP_NL_VERMINOR @UPOLICY_NL_VERMINOR@

/** full netlink version */
#define UP_NL_VERSION \
  (UP_NL_VERMAJOR << 16 | UP_NL_VERMINOR)

#endif /* _KUPOLICY_NETLINK_VERSION_H */
